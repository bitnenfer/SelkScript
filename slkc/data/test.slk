struct vec2 {
    float x;
    float y;
}
vec2 new_vec2(float x, float y) {
    vec2 v;
    v.x = x;
    v.y = y;
    return v;
}
void main() {
    int a = 3.14;
    int b = 100 + a;
    vec2 a = {100, 100};
}
void onProcess(ref vec2 velocity, const vec2 acceleration) {
    velocity.x += acceleration.x;
    velocity.y += acceleration.y;
}
